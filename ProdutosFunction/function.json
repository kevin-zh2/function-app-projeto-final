{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "{*path}",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
